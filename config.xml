<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.phonegap.example"
        versionCode = "10" 
        version     = "1.0.0" >
   <!--versionCode is optional and Android only -->
   <name>3er parcial 5toE Piano</name>
   
   <description>
   Utilizar Plugings de audio con un piano y las notas musicales
   </description>
   
   <preference name"phonegap-version" value="3.1.0" />
<!-- forzar la orientacion a horizontal -->   
   <preference  name"orientacion" value="landscape" />
   
   <author href="https://build.phonegap.com" email="support@phonegap.com">
   Andrea Mendez Lozano
   </author>
   
   <gap:plugin name="org.apache.cordova.vidration" version="0.3.0" />
   <gap:plugin name="org.apache.cordova.dialogs" version="0.2.6" />
   <gap:plugin name="com.rjfun.cordova.pluging.lowlatencyaudio" version="1.0" />
   
</widget>
